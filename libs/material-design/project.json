{
  "projectType": "library",
  "sourceRoot": "libs/material-design/src",
  "prefix": "starter",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/material-design"],
      "options": {
        "jestConfig": "libs/material-design/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/material-design/**/*.ts",
          "libs/material-design/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
